{
    "name": "carbon/link",
    "type": "neos-plugin",
    "license": "MIT",
    "description": "Link helper for Neos CMS",
    "keywords": ["flow", "neos", "fusion", "helper", "link"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "~3.0 || ~4.0"
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Link\\": "Classes"
        }
    },
    "archive": {
        "exclude": [".editorconfig", ".github", ".gitattributes", "CODE_OF_CONDUCT.md"]
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.Link",
        "neos": {
            "package-key": "Carbon.Link"
        }
    }
}
